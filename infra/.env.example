# Copy to infra/.env and override values when needed.

POSTGRES_DB=postgres
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres

# SQLAlchemy/asyncpg DSN used by app.db_app
DATABASE_URL=postgresql+asyncpg://postgres:postgres@postgres:5432/postgres

# run_app.py --mode value (all | pipeline | dashboard)
APP_MODE=all

# Port exposed by the Gradio dashboard
APP_HTTP_PORT=7860

# Set to 1 to skip DB bootstrap (useful if DB is managed elsewhere)
SKIP_DB_BOOTSTRAP=0
